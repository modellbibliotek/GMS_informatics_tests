definition
	CLUSTER[at0000] matches {    -- Phenopacket IscnAllele
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- id
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0002] matches {    -- iscn
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

